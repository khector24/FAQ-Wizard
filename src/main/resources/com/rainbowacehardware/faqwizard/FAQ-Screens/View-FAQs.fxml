            <FontAwesomeIconView glyphName="PLUS" />
         </graphic>
      </Button>
      <Button fx:id="editBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false">
         <graphic>
            <FontAwesomeIconView glyphName="EDIT" />
         </graphic>
      </Button>
      <Button fx:id="deleteBtn" layoutX="10.0" layoutY="35.0" mnemonicParsing="false">
         <graphic>
            <FontAwesomeIconView glyphName="TRASH" />
         </graphic>
      </Button>
      <Button fx:id="homeBtn" mnemonicParsing="false">
         <graphic>
            <FontAwesomeIconView glyphName="HOME" />
         </graphic>
      </Button>
      <Pane prefHeight="5.0" prefWidth="10.0" style="-fx-background-color: blue;" />
   </VBox>
   <Pane prefHeight="10.0" prefWidth="600.0" style="-fx-background-color: blue;" />
