<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../Styles/FAQ-Screens/FAQ-View.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rainbowacehardware.faqwizard.controllers.FAQScreenControllers.ViewFAQsController">
   <Text layoutX="14.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Frequently Asked Questions">
      <font>
         <Font size="26.0" />
      </font>
   </Text>
   <TableView layoutX="14.0" layoutY="54.0" prefHeight="325.0" prefWidth="533.0">
      <columns>
         <TableColumn fx:id="idCol" prefWidth="40.0" text="ID" />
